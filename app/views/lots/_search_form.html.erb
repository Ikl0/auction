<%= search_form_for @q do |f| %>


  <%= f.label :name_cont, style: 'margin: auto 1vw'%>
  <%= f.search_field :name_cont, placeholder: 'Name', style: 'margin: auto 1vw'%>
  <%= f.submit "Search", style: 'margin: auto 1vw', class:"btn btn-outline-info btn-sm" %>
  <%= link_to 'Reset', lots_path, class:"btn btn-outline-dark btn-sm" %>


<% end %>
